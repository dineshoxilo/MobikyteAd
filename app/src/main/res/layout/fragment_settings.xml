<com.desarrollodroide.libraryfragmenttransactionextended.SlidingRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <!--<android.support.v7.widget.Toolbar-->
    <!--android:id="@+id/toolbar"-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="?attr/actionBarSize"-->
    <!--android:background="@color/colorPrimary"-->
    <!--android:minHeight="?attr/actionBarSize"-->
    <!--app:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"-->
    <!--app:popupTheme="@style/ThemeOverlay.AppCompat.Dark.ActionBar" />-->

    <ProgressBar
        android:id="@+id/login_progress"
        style="?android:attr/progressBarStyleLarge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="8dp"
        android:visibility="gone"/>
    <android.support.v4.widget.NestedScrollView
        android:id="@+id/login_form"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="16dp">
        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:id="@+id/RelativeLayout01"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"
                android:paddingBottom="10dp">
                <com.oxilo.mobikyte.ui.CustomTextView
                    android:id="@+id/name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@android:color/black"
                    android:layout_centerVertical="true"
                    android:text="Name"
                    android:layout_alignParentLeft="true"/>
                <EditText
                    android:id="@+id/action_view_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimaryDark"
                    android:layout_toRightOf="@+id/name"
                    android:layout_marginLeft="5dp"
                    android:textAppearance="?android:textAppearanceSmall"
                    android:gravity="right|center"
                    android:layout_centerVertical="true"
                    android:background="@android:color/transparent"
                    android:text="Name"
                    android:layout_alignParentRight="true"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/RelativeLayout01"
                android:background="@android:color/darker_gray"></View>
            <RelativeLayout
                android:id="@+id/RelativeLayout02"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_below="@id/RelativeLayout01">

                <com.oxilo.mobikyte.ui.CustomTextView
                    android:id="@+id/bus_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@android:color/black"
                    android:layout_centerVertical="true"
                    android:text="Business Name"
                    android:layout_alignParentLeft="true"/>

                <EditText
                    android:id="@+id/action_bussiness_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimaryDark"
                    android:layout_toRightOf="@+id/bus_name"
                    android:layout_marginLeft="5dp"
                    android:gravity="right|center"
                    android:imeOptions="actionNext"
                    android:singleLine="true"
                    android:textAppearance="?android:textAppearanceSmall"
                    android:layout_centerVertical="true"
                    android:background="@android:color/transparent"
                    android:text="Name"
                    android:layout_alignParentRight="true"/>

            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/RelativeLayout02"
                android:background="@android:color/darker_gray"></View>
            <RelativeLayout
                android:id="@+id/RelativeLayout03"
                android:layout_below="@+id/RelativeLayout02"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"
                android:paddingBottom="10dp">

                <com.oxilo.mobikyte.ui.CustomTextView
                    android:id="@+id/user_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@android:color/black"
                    android:layout_centerVertical="true"
                    android:text="UserName"
                    android:layout_alignParentLeft="true"/>

                <EditText
                    android:id="@+id/action_user_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimaryDark"
                    android:layout_toRightOf="@+id/user_name"
                    android:layout_marginLeft="5dp"
                    android:gravity="right|center"
                    android:textAppearance="?android:textAppearanceSmall"
                    android:layout_centerVertical="true"
                    android:background="@android:color/transparent"
                    android:text="Name"
                    android:imeOptions="actionNext"
                    android:singleLine="true"
                    android:layout_alignParentRight="true"/>

            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/RelativeLayout03"
                android:background="@android:color/darker_gray"></View>
            <RelativeLayout
                android:id="@+id/RelativeLayout04"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_below="@+id/RelativeLayout03">

                <com.oxilo.mobikyte.ui.CustomTextView
                    android:id="@+id/buss_address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@android:color/black"
                    android:layout_centerVertical="true"
                    android:text="Business Address"
                    android:layout_alignParentLeft="true"/>

                <EditText
                    android:id="@+id/action_bussiness_address"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimaryDark"
                    android:layout_toRightOf="@+id/buss_address"
                    android:layout_marginLeft="5dp"
                    android:textAppearance="?android:textAppearanceSmall"
                    android:gravity="right|center"
                    android:layout_centerVertical="true"
                    android:background="@android:color/transparent"
                    android:text="Name"
                    android:imeOptions="actionNext"
                    android:singleLine="true"
                    android:layout_alignParentRight="true"/>
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/RelativeLayout04"
                android:background="@android:color/darker_gray"></View>
            <RelativeLayout
                android:id="@+id/RelativeLayout05"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_below="@+id/RelativeLayout04">

                <com.oxilo.mobikyte.ui.CustomTextView
                    android:id="@+id/user_city"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@android:color/black"
                    android:layout_centerVertical="true"
                    android:text="City"
                    android:layout_alignParentLeft="true"/>

                <EditText
                    android:id="@+id/action_user_city"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimaryDark"
                    android:textAppearance="?android:textAppearanceSmall"
                    android:layout_toRightOf="@+id/user_city"
                    android:layout_marginLeft="5dp"
                    android:gravity="right|center"
                    android:imeOptions="actionNext"
                    android:singleLine="true"
                    android:layout_centerVertical="true"
                    android:background="@android:color/transparent"
                    android:text="Name"
                    android:layout_alignParentRight="true"/>

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/RelativeLayout05"
                android:background="@android:color/darker_gray"></View>
            <RelativeLayout
                android:id="@+id/RelativeLayout06"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_below="@+id/RelativeLayout05">

                <com.oxilo.mobikyte.ui.CustomTextView
                    android:id="@+id/state"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@android:color/black"
                    android:layout_centerVertical="true"
                    android:text="state"
                    android:layout_alignParentLeft="true"/>

                <EditText
                    android:id="@+id/action_user_state"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimaryDark"
                    android:layout_toRightOf="@+id/state"
                    android:layout_marginLeft="5dp"
                    android:textAppearance="?android:textAppearanceSmall"
                    android:gravity="right|center"
                    android:layout_centerVertical="true"
                    android:imeOptions="actionNext"
                    android:singleLine="true"
                    android:background="@android:color/transparent"
                    android:text="Name"
                    android:layout_alignParentRight="true"/>


            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/RelativeLayout06"
                android:background="@android:color/darker_gray"></View>
            <RelativeLayout
                android:id="@+id/RelativeLayout07"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_below="@+id/RelativeLayout06">

                <com.oxilo.mobikyte.ui.CustomTextView
                    android:id="@+id/zip_code"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@android:color/black"
                    android:layout_centerVertical="true"
                    android:text="Zip Code"
                    android:layout_alignParentLeft="true"/>
                <EditText
                    android:id="@+id/action_zip_code"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimaryDark"
                    android:layout_toRightOf="@+id/zip_code"
                    android:layout_marginLeft="5dp"
                    android:imeOptions="actionNext"
                    android:singleLine="true"
                    android:gravity="right|center"
                    android:textAppearance="?android:textAppearanceSmall"
                    android:layout_centerVertical="true"
                    android:background="@android:color/transparent"
                    android:text="Name"
                    android:layout_alignParentRight="true"/>

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/RelativeLayout07"
                android:background="@android:color/darker_gray"></View>
            <RelativeLayout
                android:id="@+id/RelativeLayout08"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_below="@+id/RelativeLayout07">

                <com.oxilo.mobikyte.ui.CustomTextView
                    android:id="@+id/email"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@android:color/black"
                    android:layout_centerVertical="true"
                    android:text="Email"
                    android:layout_alignParentLeft="true"/>

                <EditText
                    android:id="@+id/action_email"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimaryDark"
                    android:layout_toRightOf="@+id/email"
                    android:layout_marginLeft="5dp"
                    android:textAppearance="?android:textAppearanceSmall"
                    android:gravity="right|center"
                    android:clickable="false"
                    android:cursorVisible="false"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:imeOptions="actionNext"
                    android:singleLine="true"
                    android:layout_centerVertical="true"
                    android:background="@android:color/transparent"
                    android:text="Name"
                    android:layout_alignParentRight="true"/>

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/RelativeLayout08"
                android:background="@android:color/darker_gray"></View>
            <RelativeLayout
                android:id="@+id/RelativeLayout09"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_below="@+id/RelativeLayout08">

                <com.oxilo.mobikyte.ui.CustomTextView
                    android:id="@+id/website"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@android:color/black"
                    android:layout_centerVertical="true"
                    android:text="Your website's url"
                    android:layout_alignParentLeft="true"/>
                <EditText
                    android:id="@+id/action_web_url"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimaryDark"
                    android:layout_toRightOf="@+id/website"
                    android:layout_marginLeft="5dp"
                    android:gravity="right|center"
                    android:imeOptions="actionNext"
                    android:singleLine="true"
                    android:textAppearance="?android:textAppearanceSmall"
                    android:layout_centerVertical="true"
                    android:background="@android:color/transparent"
                    android:text="Name"
                    android:layout_alignParentRight="true"/>

            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="5dp"
                android:layout_below="@+id/RelativeLayout09"
                android:background="@android:color/darker_gray"></View>
            <RelativeLayout
                android:id="@+id/RelativeLayout10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_below="@+id/RelativeLayout09">

                <com.oxilo.mobikyte.ui.CustomTextView
                    android:id="@+id/mobile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@android:color/black"
                    android:layout_centerVertical="true"
                    android:text="Mobile Number"
                    android:layout_alignParentLeft="true"/>
                <EditText
                    android:id="@+id/action_mobile_no"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimaryDark"
                    android:layout_toRightOf="@+id/mobile"
                    android:textAppearance="?android:textAppearanceSmall"
                    android:layout_marginLeft="5dp"
                    android:imeOptions="actionNext"
                    android:singleLine="true"
                    android:gravity="right|center"
                    android:layout_centerVertical="true"
                    android:background="@android:color/transparent"
                    android:text="Name"
                    android:layout_alignParentRight="true"/>

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/RelativeLayout10"
                android:background="@android:color/darker_gray"></View>
            <RelativeLayout
                android:id="@+id/RelativeLayout11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_below="@+id/RelativeLayout10">

                <com.oxilo.mobikyte.ui.CustomTextView
                    android:id="@+id/about"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@android:color/black"
                    android:layout_centerVertical="true"
                    android:text="About"
                    android:layout_alignParentLeft="true"/>

                <EditText
                    android:id="@+id/action_about"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimaryDark"
                    android:layout_toRightOf="@+id/about"
                    android:layout_marginLeft="5dp"
                    android:imeOptions="actionDone"
                    android:singleLine="true"
                    android:textAppearance="?android:textAppearanceSmall"
                    android:gravity="right|center"
                    android:layout_centerVertical="true"
                    android:background="@android:color/transparent"
                    android:text="Name"
                    android:layout_alignParentRight="true"/>

            </RelativeLayout>
            <View
                android:id="@+id/view_11"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/RelativeLayout11"
                android:background="@android:color/darker_gray"></View>

            <android.support.v7.widget.AppCompatButton
                android:id="@+id/email_sign_in_button"
                style="?android:textAppearanceSmall"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:layout_below="@+id/view_11"
                android:text="@string/action_update"
                android:textColor="@android:color/white"
                android:textStyle="bold" />


        </RelativeLayout>
    </android.support.v4.widget.NestedScrollView>
</com.desarrollodroide.libraryfragmenttransactionextended.SlidingRelativeLayout>
